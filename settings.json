{
    "openai": {
        "instances": [
            {
                "email": "email1@email.com",
                "password": "password1",
                "plus": true
            },
            {
                "email": "email2@email.com",
                "password": "password2",
                "plus": false
            }
        ],
        "access_token_refresh_interval": 86400,
        "base_url": "https://chat.openai.com/backend-api",
        "endpoints": {
            "conversation": "/conversation",
            "gen_title": "/conversation/gen_title"
        }
    },
    "api_server": {        
        "webui_language": "en-US",
        "host": "0.0.0.0",
